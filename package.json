{
  "name": "ms-video-indexer",
  "version": "0.0.15",
  "description": "Unofficial Javascript client library for Microsoft Video Indexer",
  "repository": {
    "type": "git",
    "url": "nwaughachukwuma/ms-video-indexer-js",
    "directory": "https://github.com/nwaughachukwuma"
  },
  "homepage": "https://github.com/nwaughachukwuma/ms-video-indexer-js#readme",
  "license": "MIT",
  "author": {
    "name": "Chukwuma Nwaugha",
    "email": "nwaughac@gmail.com",
    "url": "https://nwaughachukwuma.github.io/portfolio"
  },
  "type": "module",
  "main": "./dist/index.js",
  "sideEffects": [
    "./dist/index.js"
  ],
  "keywords": [
    "Video AI",
    "Video Insights",
    "Decode Video",
    "Video Analyzer"
  ],
  "exports": "./index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "tsc": "tsc",
    "test": "yarn jest",
    "lint": "eslint --fix index.js",
    "prettify": "yarn lint && prettier --write index.js"
  },
  "dependencies": {
    "node-fetch": "^3.0.0",
    "sma-cache": "^0.0.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/jest-matcher-one-of": "^1.0.3",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/parser": "4.17.0",
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.0",
    "jest": "^26.6.3",
    "jest-matcher-one-of": "^1.0.2",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "ts-jest": "^26.5.3",
		"typescript": "^4.4.3"
  },
  "lint-staged": {
    "./**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "./**/*.spec.{ts,js}": [
      "yarn test --collectCoverage=false"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "preset": "ts-jest",
    "reporters": [
      "default"
    ],
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/jest.setup.ts"
    ]
  }
}
